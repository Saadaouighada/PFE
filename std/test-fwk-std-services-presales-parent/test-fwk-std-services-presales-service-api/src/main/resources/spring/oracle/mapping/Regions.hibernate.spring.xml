<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.linedata.presales.entities.impl.RegionsImpl"
		table="REGIONS">
		<id name="idRegion" type="long" column="ID_REGION">
		<generator class="sequence">
		<param name="sequence_name">SEQREGION_ID</param>    
		</generator>
		</id>
		<property name="idExterne" column="ID_Externe" type="string" />
		<property name="codeRegion" column="CODE_REGION" type="string" />
		<property name="codeStatus" column="CODE_STATUS" type="string" />
	</class>
</hibernate-mapping>  